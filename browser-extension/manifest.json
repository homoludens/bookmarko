{
  "manifest_version": 3,
  "name": "Flaskmarks - Save Tabs",
  "version": "1.0.0",
  "description": "Save all open tabs or current tab as bookmarks to your Flaskmarks instance",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/api/v1/*",
    "https://*/api/v1/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
